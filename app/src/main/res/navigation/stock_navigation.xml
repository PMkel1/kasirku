<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/stock_navigation"
    app:startDestination="@id/stockFragment">

    <fragment
        android:id="@+id/stockFragment"
        android:name="com.thoriq.kasirku.stock.StockFragment"
        android:label="StockFragment" >
        <action
            android:id="@+id/action_stockFragment_to_detailStockFragment"
            app:destination="@id/detailStockFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailStockFragment"
        android:name="com.thoriq.kasirku.DetailStockFragment"
        android:label="fragment_detail_stock"
        tools:layout="@layout/fragment_detail_stock" />
</navigation>